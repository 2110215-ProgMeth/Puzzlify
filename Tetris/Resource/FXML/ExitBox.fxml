<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.ExitBox">
   <children>
      <ImageView fitHeight="377.0" fitWidth="502.0" layoutX="49.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../UISprite/GamOverMenu.png" />
         </image>
      </ImageView>
      <ImageView fx:id="exitBtn" fitHeight="138.0" fitWidth="141.0" layoutX="330.0" layoutY="231.0" pickOnBounds="true" preserveRatio="true" onMousePressed="#OnExitBtnPressed">
         <image>
            <Image url="@../UISprite/Exit.png" />
         </image>
      </ImageView>
      <Text fx:id="ScoreTxt" fill="WHITE" layoutX="135.0" layoutY="178.0" strokeType="OUTSIDE" strokeWidth="0.0" text="SCORE :">
         <font>
            <Font size="33.0" />
         </font>
      </Text>
      <Text fx:id="LineTxt" fill="WHITE" layoutX="135.0" layoutY="222.0" strokeType="OUTSIDE" strokeWidth="0.0" text="LINE :">
         <font>
            <Font size="33.0" />
         </font>
      </Text>
      <ImageView fx:id="restartBtn" fitHeight="121.0" fitWidth="115.0" layoutX="147.0" layoutY="242.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../UISprite/ReStart.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
